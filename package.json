{
  "name": "gosling",
  "version": "1.1.0",
  "description": "Gosling is a simple es6 node server, similar to express.",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "rm -f dist/*",
    "build": "babel src -d dist",
    "pretest": "eslint src/",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --watch --reporter min --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tbremer/gosling.git"
  },
  "keywords": [
    "webserver",
    "server",
    "express"
  ],
  "author": "Tom Bremer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tbremer/gosling/issues"
  },
  "homepage": "https://github.com/tbremer/gosling#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.0.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "eslint": "^2.11.1",
    "eslint-config-tbremer": "^1.2.0",
    "eslint-plugin-babel": "^3.2.0",
    "expect": "^1.20.1",
    "mocha": "^2.5.3",
    "pre-commit": "^1.1.3"
  },
  "pre-commit": [
    "test"
  ]
}
